{
    "language": "go",
    "env": "gomod",
    "description": "go语言grpc服务和sdk的模板库.",
    "author": "hsz",
    "template_type": "socket",
    "requires": [
        "github.com/Golang-Tools/loggerhelper@v0.0.3",
        "github.com/Golang-Tools/schema-entry-go@v0.0.4",
        "github.com/golang/protobuf@v1.5.2",
        "github.com/json-iterator/go@v1.1.11",
        "github.com/liyue201/grpc-lb@v0.0.0-20201117062843-867c3e20933f",
        "google.golang.org/grpc@v1.38.0",
        "google.golang.org/protobuf@v1.26.0"
    ],
    "template_keys": {
        "serv_name": {
            "description": "定义service的名字",
            "default": "{{ project_name }}",
            "ask": true
        },
        "app_name": {
            "description": "定义app的名字",
            "default": "{{ project_name }}"
        }
    },
    "components": {
        "sdk": {
            "source": "tp_go_grpc//sdk",
            "description": "grpc的客户端定义",
            "default_path": "{{ serv_name }}_sdk"
        },
        "serv": {
            "source": "tp_go_grpc//serv",
            "description": "grpc的服务端定义",
            "default_path": "{{ serv_name }}_serv"
        },
        "main": {
            "source": "tp_go_grpc//main",
            "description": "入口文件",
            "default_path": "main.go"
        },
        "service_schema": {
            "source": "tp_go_grpc//service_schema",
            "description": "项目定义service的proto文件",
            "default_path": "pbschema/{{ serv_name }}.proto"
        }
    }
}